{
    "name": "@thumbtack/thumbprint-tokens",
    "description": "Design variables that power Thumbtackâ€™s UI.",
    "version": "12.1.1",
    "main": "dist/javascript-cjs/index.js",
    "module": "dist/javascript-es/index.js",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "scripts": {
        "start": "yarn clear && yarn build",
        "build:main": "ts-node ./src/index.ts",
        "build:esm": "tsc --project ./tsconfig-build.json --module es2015 --target ES2015 --outDir dist/javascript-es",
        "build:cjs": "tsc --project ./tsconfig-build.json --module commonjs --target ES2015 --outDir dist/javascript-cjs",
        "build": "yarn build:main && yarn build:esm && yarn build:cjs",
        "clear": "rm -rf dist",
        "typecheck": "tsc --noEmit",
        "lint:js": "eslint . --ext .jsx --ext .js",
        "lint:prettier": "prettier \"**/*.{js,jsx,css,scss,json,md,mdx,html}\" --check",
        "test": "jest",
        "static": "yarn typecheck && yarn lint:js && yarn lint:prettier",
        "verify": "yarn netlify-zip-it-ship-it-issue-38-workaround && yarn start && yarn static && yarn test",
        "netlify-zip-it-ship-it-issue-38-workaround": "cp src/tokens.ts functions/graphql/tokens-copy.ts && cp package.json functions/graphql/package-copy.json",
        "prepublish": "yarn start",
        "release": "np --no-release-draft"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "author": "Daniel O'Connor <daniel@danoc.me>",
    "license": "Apache-2.0",
    "devDependencies": {
        "@types/fs-extra": "^9.0.13",
        "@types/jest": "^27.5.1",
        "@types/jszip": "^3.4.1",
        "@typescript-eslint/eslint-plugin": "^5.26.0",
        "@typescript-eslint/parser": "^5.26.0",
        "eslint": "^8.16.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^26.2.2",
        "fs-extra": "^10.1.0",
        "handlebars": "^4.2.0",
        "husky": "^3.0.9",
        "jest": "^24.9.0",
        "jszip": "^3.10.0",
        "lodash": "^4.17.4",
        "np": "^5.2.1",
        "prettier": "^2.6.2",
        "pretty-quick": "^2.0.0",
        "ts-jest": "^28.0.3",
        "ts-node": "^10.8.0",
        "typescript": "^4.7.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thumbtack/thumbprint-tokens.git"
    },
    "bugs": {
        "url": "https://github.com/thumbtack/thumbprint-tokens/issues"
    },
    "homepage": "https://thumbprint.design/tokens",
    "dependencies": {
        "apollo-server-lambda": "^2.9.7",
        "graphql": "^14.5.8"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "packageManager": "yarn@3.2.1"
}
