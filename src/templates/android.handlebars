<?xml version="1.0" encoding="utf-8"?>
<resources>
{{#*inline "sectionPartial"}}
    {{#each tokens as |token|}}
        {{#if token.platforms.android}}
            {{#if token.platforms.android.description}}
    <!-- {{token.platforms.android.description}} -->
            {{/if}}
            {{#if token.deprecated}}
    <!-- Deprecated -->
            {{/if}}
    <{{getAndroidTag token.format}} name="{{token.platforms.android.name}}">{{token.platforms.android.value}}</{{getAndroidTag token.format}}>
        {{/if}}
    {{/each}}
    {{#each subcategories}}
        {{~> sectionPartial}}
    {{/each}}
{{/inline ~}}

{{#each this}}
    {{~> sectionPartial}}
{{/each}}
</resources>
