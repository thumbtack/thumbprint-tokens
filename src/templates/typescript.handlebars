{{#*inline "sectionPartial"}}
    {{#each tokens as |token|}}
        {{#if token.platforms.javascript}}
            {{#if token.platforms.javascript.description ~}}
                // {{token.platforms.javascript.description}}
            {{/if}}
            {{#if token.deprecated ~}}
                // @deprecated
            {{/if ~}}
            export var {{token.platforms.javascript.name}} = {{#isString token.platforms.javascript.value}}"{{/isString}}{{{token.platforms.javascript.value}}}{{#isString token.platforms.javascript.value}}"{{/isString}};
        {{/if}}
    {{/each}}
    {{#each subcategories}}
        {{~> sectionPartial}}
    {{/each}}
{{/inline ~}}

{{#each this}}
    {{~> sectionPartial}}
{{/each}}
